                    <rdf:Description rdf:about="&obo;IAO_0000122"/>
                    <rdf:Description rdf:about="&obo;IAO_0000123"/>
                    <rdf:Description rdf:about="&obo;IAO_0000428"/>
                    <rdf:Description rdf:about="&obo;IAO_0000120"/>
