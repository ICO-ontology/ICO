        <IAO_0000117 xml:lang="en">J. Neil Otte</IAO_0000117>
        <IAO_0000119 rdf:resource="http://purl.obolibrary.org/obo/HP_0100543"/>
        <IAO_0000232 xml:lang="en">Adapted from http://purl.obolibrary.org/obo/HP_0100543</IAO_0000232>
        <dc:date xml:lang="en">Imported and adapted 8.22.2018</dc:date>
        <rdfs:label xml:lang="en">cognitive impairment</rdfs:label>
    </owl:Class>
    


