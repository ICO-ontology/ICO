    <owl:Class rdf:about="http://purl.obolibrary.org/obo/ICO_0000302">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/ICO_0000269"/>
        <obo:IAO_0000115 xml:lang="en">An act of communicating in which information is shared, overriding or violating a judgment that the information should not be shared.</obo:IAO_0000115>
