        <obo:IAO_0000118 xml:lang="en">description of liability</obo:IAO_0000118>
        <obo:ICO_0000231 xml:lang="en"></obo:ICO_0000231>
        <rdfs:label xml:lang="en">description of legal responsibility</rdfs:label>
    </owl:Class>
    


