    </owl:Class>
    


    <!-- http://purl.obolibrary.org/obo/ICO_0000094 -->

    <owl:Class rdf:about="http://purl.obolibrary.org/obo/ICO_0000094">
        <rdfs:subClassOf rdf:resource="http://www.geneontology.org/formats/oboInOwl#ObsoleteClass"/>
        <obo:IAO_0000115 xml:lang="en">The process by which information about the health status of an individual is obtained after a study has officially closed; an activity that continues something that has already begun or that repeats something that has already been done.</obo:IAO_0000115>
        <obo:IAO_0000117 xml:lang="en">Asiyah Yu Lin</obo:IAO_0000117>
        <obo:IAO_0000119 xml:lang="en">NCIT:C16033</obo:IAO_0000119>
        <obo:IAO_0000232 xml:lang="en">Elizabeth: Pay attention to definition &quot;after study procedures and treatments have concluded.&quot; Does it fit our purposes?</obo:IAO_0000232>
        <obo:ICO_0000092 xml:lang="en">C16033</obo:ICO_0000092>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">obsolete follow-up visit</rdfs:label>
        <owl:deprecated rdf:datatype="http://www.w3.org/2001/XMLSchema#boolean">true</owl:deprecated>
    </owl:Class>
    


    <!-- http://purl.obolibrary.org/obo/ICO_0000095 -->

    <owl:Class rdf:about="http://purl.obolibrary.org/obo/ICO_0000095">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/ICO_0000078"/>
        <obo:IAO_0000115 xml:lang="en">A risk that is equal or lesser than those ordinarily encountered in daily life or during the performance of routine physical or psychological examinations or tests.</obo:IAO_0000115>
        <obo:IAO_0000117 xml:lang="en">Asiyah Yu Lin, J. Neil Otte, Cooper Stansbury</obo:IAO_0000117>
        <obo:IAO_0000119 xml:lang="en">(45 CFR 46.102(i)).  Available online from: http://www.hhs.gov/ohrp/humansubjects/guidance/45cfr46.html#46.102 Accessed 7/31/13.</obo:IAO_0000119>
        <obo:IAO_0000232 xml:lang="en">Definition varies slightly for research on prisoners; see (45 CFR 46.303(d)).</obo:IAO_0000232>
        <rdfs:label xml:lang="en">minimal risk</rdfs:label>
    </owl:Class>
    


    <!-- http://purl.obolibrary.org/obo/ICO_0000096 -->

