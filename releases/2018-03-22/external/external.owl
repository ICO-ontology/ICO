        <rdfs:subClassOf rdf:resource="&obo;IAO_0000310"/>
        <dc:source>NCIT</dc:source>
        <obo:IAO_0000115 xml:lang="en">A document that protects researchers from involuntary disclosure of personal identifying information acquired in the course of a research project. Data cannot be subpoenaed and assures anonymity to study participants.</obo:IAO_0000115>
        <obo:IAO_0000412>http://ncicb.nci.nih.gov/xml/owl/EVS/Thesaurus.owl</obo:IAO_0000412>
