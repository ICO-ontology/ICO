        <rdfs:subClassOf rdf:resource="&obo;IAO_0000300"/>
        <obo:IAO_0000115 xml:lang="en">A word or group of words indicating the identity of a person usually consisting of a first (personal) name and a last (family) name with an optional middle name. In some cultural traditions the family name comes first.</obo:IAO_0000115>
        <obo:IAO_0000412>http://ncicb.nci.nih.gov/xml/owl/EVS/Thesaurus.owl</obo:IAO_0000412>
        <dc:source>NCIT</dc:source>
    </owl:Class>
    


    <!-- http://ncicb.nci.nih.gov/xml/owl/EVS/Thesaurus.owl#C25678 -->

