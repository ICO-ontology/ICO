        <rdfs:subClassOf rdf:resource="&NCIT;C25678"/>
        <obo:IAO_0000115 xml:lang="en">A textual or multimedia form of the signed name or personal mark of the investigator who is responsible for completing a particular document as a part of clinical study activity. By signing this document the investigator confirms his participation and his role in the study and assumes responsibility and accountability associated with this role.</obo:IAO_0000115>
        <obo:IAO_0000412>http://ncicb.nci.nih.gov/xml/owl/EVS/Thesaurus.owl</obo:IAO_0000412>
        <dc:source>NCIT</dc:source>
    </owl:Class>
    


    <!-- http://ncicb.nci.nih.gov/xml/owl/EVS/Thesaurus.owl#C70958 -->

    <owl:Class rdf:about="&NCIT;C70958">
