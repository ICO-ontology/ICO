        <rdfs:subClassOf rdf:resource="&obo;UBERON_0000477"/>
        <obo:IAO_0000115 rdf:datatype="&xsd;string">The thin layer of gray matter on the surface of the cerebral hemisphere that develops from the telencephalon. It consists of the neocortex (6 layered cortex or isocortex), the hippocampal formation and the olfactory cortex.</obo:IAO_0000115>
        <obo:IAO_0000111 rdf:datatype="&xsd;string">cerebral cortex</obo:IAO_0000111>
        <obo:IAO_0000412 rdf:resource="&obo;obi.owl"/>
        <obo:IAO_0000412 rdf:resource="&obo;uberon.owl"/>
