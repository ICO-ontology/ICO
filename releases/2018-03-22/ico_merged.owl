    <!-- http://purl.obolibrary.org/obo/ICO_0000113 -->

    <owl:Class rdf:about="&obo;ICO_0000113">
        <rdfs:label xml:lang="en">anonymized data</rdfs:label>
        <rdfs:subClassOf rdf:resource="&obo;IAO_0000027"/>
        <obo:IAO_0000119 xml:lang="en">CDISC clinical research glossary. (2011). Applied Clinical Trials, 20(12), 11-43. Retrieved from http://search.proquest.com.proxy.lib.umich.edu/docview/914932076?accountid=14667</obo:IAO_0000119>
        <obo:IAO_0000117 xml:lang="en">YL</obo:IAO_0000117>
