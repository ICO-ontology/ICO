    <owl:Class rdf:about="&obo;OBI_0100060">
        <rdfs:label rdf:datatype="&xsd;string">cultured cell population</rdfs:label>
        <rdfs:subClassOf rdf:resource="&obo;OBI_0000047"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&obo;OBI_0000312"/>
