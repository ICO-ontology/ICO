    <!-- http://purl.obolibrary.org/obo/UBERON_0000173 -->

    <owl:Class rdf:about="&obo;UBERON_0000173">
        <rdfs:label rdf:datatype="&xsd;string">amniotic fluid</rdfs:label>
        <rdfs:subClassOf rdf:resource="&obo;UBERON_0000463"/>
        <obo:IAO_0000115 rdf:datatype="&xsd;string">Amniotic fluid is a bodily fluid consisting of watery liquid surrounding and cushioning a growing fetus within the amnion. It allows the fetus to move freely without the walls of the uterus being too tight against its body. Buoyancy is also provided. The composition of the fluid changes over the course of gestation. Initially, amniotic fluid is similar to maternal plasma, mainly water with electrolytes. As the fetus develops, proteins, carbohydrates, lipids, phospholipids originating from the lungs, fetal cells, and urea are deposited in the fluid.</obo:IAO_0000115>
        <obo:IAO_0000111 rdf:datatype="&xsd;string">amniotic fluid</obo:IAO_0000111>
        <obo:IAO_0000412 rdf:resource="&obo;obi.owl"/>
        <obo:IAO_0000412 rdf:resource="&obo;uberon.owl"/>
    </owl:Class>
    


